{
  "name": "@dap-overlay/sdk-core",
  "version": "0.1.0",
  "description": "Core types, validation, and telemetry for DAP Overlay SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "dompurify": "^3.0.6"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
